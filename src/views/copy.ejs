<!DOCTYPE html>
<html lang="en">

<head>

	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="description" content="">
	<meta name="author" content="">

	<title>UsabL</title>

	<!-- Bootstrap core CSS -->
	<link href="../../vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

	<!-- Custom fonts for this template -->
	<link href="../../vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
	<link href="https://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic" rel="stylesheet" type="text/css">
	<link href='https://fonts.googleapis.com/css?family=Cabin:700' rel='stylesheet' type='text/css'>

	<!-- Custom styles for this template -->
	<link href="../../css/grayscale.css" rel="stylesheet">

</head>

<body id="page-top">

	<!-- Navigation -->
	<nav class="navbar navbar-expand-lg navbar-light fixed-top" id="mainNav">
		<div class="container">
			<a class="navbar-brand js-scroll-trigger" href="#page-top">UsabL</a>
			<button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive"
			 aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
				Menu
				<i class="fa fa-bars"></i>
			</button>
			<div class="collapse navbar-collapse" id="navbarResponsive">

				<ul class="navbar-nav ml-auto">
					<li class="nav-item">
						<a class="nav-link" href="/auth/profile">Profile</a>
					</li>
				</ul>
			</div>
		</div>
	</nav>

	<!-- About Section -->
	<section id="start-test" class="content-section text-center">
		<div class="container">
			<div class="row">
				<div class="col-sm-2 col-12"></div>
				<div class="col-12 col-sm-8">
					<div class="card">
						<div class="card-body">
							<h5 class="card-title text-black">Copy a test</h5>
							<p class="card-text text-black">Change the original items on your test.</p>
							<form name="noLog" action="/site/" method="post">
								<div class="row text-black" style="margin-bottom: 20px">
									<div class="col-3">Name</div>
									<div class="col-9">
										<input class="form-control" name="testName" placeholder="Name of Test" value="<%=newDoc.testName%>">
									</div>
								</div>


								<%for (var i=0; i < newDoc.questionArray.length; i++) {%>

								<div class="row text-black" style="margin-bottom: 20px">
									<div class="col-3">
										<%=i+1%>
									</div>
									<div class="col-9">
										<input class="form-control" name="task<%=i%>" placeholder="Test Item" value="<%=newDoc.questionArray[i]%>">
									</div>
								</div>

								<%}%>


								<div class="row">
									<div class="col-9">
										<input placeholder="URL" name="webURL" id="webURL" class="form-control" value="<%=newDoc.webURL%>" required>
									</div>
									<div class="col-3">
										<button type="submit" id="submit" class="btn btn-default btn-sm">Submit</button>
									</div>
								</div>
							</form>
						</div>
					</div>
				</div>
				<div class="col-sm-2 col-12"></div>
			</div>
		</div>
	</section>




	<!-- Footer -->
	<footer>
		<div class="container text-center">
			<p>Copyright &copy; Your Website 2018</p>
		</div>
	</footer>


</body>

</html>